                "required": true
            },
            {
                "name": "query1",
                "in": "query",
                "type": "string",
                "description": "手机",
                "pattern":"^1[3|4|5|7|8]\\d{9}$",
                "error_tip":"请输入正确的客户手机",
                "required": true
            },
            {
                "name": "query2",
                "in": "query",
                "type": "integer",
                "description": "最大100，最小1",
                "error_tip":"请输入正确的数字，最大100，最小1",
