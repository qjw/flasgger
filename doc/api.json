            "校验"
        ],
        "parameters": [
            {
                "name": "q1",
                "in": "query",
                "type": "integer",
                "minimum": 0,
                "maximum": 100,
                "exclusiveMaximum": true,
                "description": "整形区间1&lt;=x&lt;100，不包含最大的值",
                "required": false
