    "6": {
        "summary": "字符串",
        "tags": [
            "校验"
        ],
        "parameters": [
            {
                "name": "q1",
                "in": "query",
                "type": "string",
                "minLength": 1,
                "maxLength": 6,
                "description": "字符串长度限制",
                "required": false
            },
            {
                "name": "q2",
                "in": "query",
                "type": "string",
                "pattern":"^1[3|4|5|7|8]\\d{9}$",
                "description": "正则表达式",
                "required": false
            },
            {
                "name": "q3",
                "in": "query",
                "type": "string",
                "format": "date",
                "description": "内建的date",
                "default": "2017-9-15",
                "required": false
            },
            {
                "name": "q4",
                "in": "query",
                "type": "string",
                "format": "date-time",
                "description": "内建的datetime",
                "default": "2017-04-13T14:34:23+00:00",
                "required": false
            },
            {
                "name": "q5",
                "in": "query",
                "type": "string",
                "format": "email",
                "description": "内建的email",
                "default": "qjw@qiujinwu.com",
                "required": false
            },
            {
                "name": "q6",
                "in": "query",
                "type": "string",
                "description": "字符串枚举",
                "enum": [
                    "str1",
                    "str2",
                    "str3"
                ],
                "required": false
            }
        ],
        "responses": {
            "200": {
                "schema": {
                    "properties": {
                        "code": {
                            "type": "integer",
                            "description": "返回码",
                            "default": 0
                        },
                        "message": {
                            "type": "string",
                            "description": "返回字符串描述",
                            "default": "ok"
                        }
                    }
                }
            }
        }
    },
