                        "q3":{
                            "type": "array",
                            "items": {
                                "type": "integer"
                            },
                            "description": "数组元素必须是整形"
                        },
                        "q4":{
                            "type": "array",
                            "items": [
                                {
                                    "type": "number"
                                },
                                {
                                    "type": "string",
                                    "default": "abcdefg"
                                },
                                {
                                    "type": "string",
                                    "enum": ["Street", "Avenue", "Boulevard"]
                                },
                                {
                                    "type": "string",
                                    "enum": ["NW", "NE", "SW", "SE"]
                                }
                            ],
                            "description": "限定每个元素的schema"
                        }
                    }
                }
            }
        ],
        "responses": {
            "200": {
                "schema": {
                    "properties": {
                        "code": {
