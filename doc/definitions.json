{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "car_model": {
        "description": "车辆型号",
        "type": "object",
        "properties": {
            "brand": {
                "type": "string",
                "description": "汽车品牌",
                "default": "奔驰"
            },
            "series": {
                "type": "string",
                "description": "汽车系列",
                "default": "S600"
            },
            "model": {
                "type": "string",
                "description": "汽车型号",
                "default": "尊贵型"
            }
        },
        "required": [
            "brand"
        ]
    }
}